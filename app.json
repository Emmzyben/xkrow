{
  "expo": {
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "name": "xkrow",
    "slug": "xkrow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#62248F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.emiblaq.xkrow",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8be3faee-0c4a-4b76-901a-f7641a88d86a"
      }
    },
    "owner": "emiblaq"
  }
}
